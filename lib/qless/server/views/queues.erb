<div class="page-header">
	<h1>Known Queues <small>And their job counts</small></h1>
</div>

<% queues.each do |queue| %>
<div class="row">
	<div class="span3">
		<h1><a href="/queues/<%= queue['name'] %>"><%= queue['name'] %></a></h1>
	</div>
	<div class="span9">
		<h1> |
			<%= queue['running']   %> /
			<%= queue['waiting']   %> /
			<%= queue['scheduled'] %> /
			<%= queue['stalled']   %> <small>(running / waiting / scheduled / stalled)</small>
		</h1>
	</div>
</div>
<% end %>
